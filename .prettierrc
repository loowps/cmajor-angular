# Prettier configuration

printWidth: 100
singleQuote: true
tabWidth: 2
useTabs: false

# js and ts rules:
arrowParens: avoid

# jsx and tsx rules:
bracketSameLine: false

semi: true
trailingComma: all
bracketSpacing: true
#rangeStart: 0
#rangeEnd: Infinity
requirePragma: false
insertPragma: false
proseWrap: preserve
